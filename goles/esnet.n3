@prefix esnet: <urn:ogf:network:esnet:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:startap:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/startap.n3" .

esnet:bi-esnet-startap a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort esnet:esnet-startap,
        esnet:startap-esnet .

esnet:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort esnet:ps-in,
        esnet:ps-out .

esnet:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort esnet:bi-esnet-startap,
        esnet:bi-ps;
    nml:hasInboundPort esnet:ps-in,
        esnet:startap-esnet;
    nml:hasOutboundPort esnet:esnet-startap,
        esnet:ps-out;
    nml:locatedAt esnet:location;
    nsi:managedBy esnet:nsa .

esnet:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "41.703516"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "-87.988120"^^<http://www.w3.org/2001/XMLSchema#float> .

esnet:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: OSCARS 
            Project Manager: Inder Monga 
            Email: imonga@es.net 
            Software development engineer: Evangelos Chaniotakis 
            Email:  haniotak@es.net
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://jupiter.es.net:8288/ConnectionService
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing esnet:ets .

esnet:esnet-startap a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap:2012:esnet-startap>;
    nmleth:vlans "1780-1783" .

esnet:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

esnet:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

esnet:startap-esnet a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap:2012:startap-esnet>;
    nmleth:vlans "1780-1783" .

