@prefix esnet: <urn:ogf:network:esnet.net:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

esnet:bi-esnet-None a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort esnet:ps-in,
        esnet:ps-out .

esnet:bi-esnet-starlight a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort esnet:esnet-starlight,
        esnet:starlight-esnet .

<urn:ogf:network:starlight.net:2012:topology> a nml:Topology,
        owl:NamedIndividual;
    nml:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/starlight.n3" .

esnet:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "41.703516"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "-87.988120"^^<http://www.w3.org/2001/XMLSchema#float> .

esnet:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: OSCARS 
            Project Manager: Inder Monga 
            Email: imonga@es.net 
            Software development engineer: Evangelos Chaniotakis 
            Email:  haniotak@es.net
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://jupiter.es.net:8288/ConnectionService
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing esnet:topology .

esnet:topology a nml:Topology,
        owl:NamedIndividual;
    nml:hasInboundPort esnet:ps-in,
        esnet:starlight-esnet;
    nml:hasOutboundPort esnet:esnet-starlight,
        esnet:ps-out;
    nml:locatedAt esnet:location;
    nsi:managedBy esnet:nsa .

esnet:esnet-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:starlight.net:2012:esnet-starlight>;
    nmleth:vlans "1780-1783" .

esnet:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

esnet:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

esnet:starlight-esnet a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:starlight.net:2012:starlight-esnet>;
    nmleth:vlans "1780-1783" .

