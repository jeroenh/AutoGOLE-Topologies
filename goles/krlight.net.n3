@prefix krlight: <urn:ogf:network:krlight.net:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:gloriad.org:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/gloriad.org.n3" .

krlight:bi-krlight-gloriad a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort krlight:gloriad-krlight,
        krlight:krlight-gloriad .

krlight:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort krlight:ps-in,
        krlight:ps-out .

krlight:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort krlight:gloriad-krlight,
        krlight:ps-in ;
    nml:hasOutboundPort krlight:krlight-gloriad,
        krlight:ps-out ;
    nml:locatedAt krlight:location ;
    nsi:managedBy krlight:nsa .

krlight:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "36.366"^^xsd:float ;
    nml:long "127.359"^^xsd:float .

krlight:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
