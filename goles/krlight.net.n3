            : SC2011 booth#  656  KISTI
        """^^xsd:string ;
    nsi:csProviderEndpoint """
            http://203.230.116.202:8090/NSI_v2.0/ConnectionServiceProvider
        """^^xsd:string ;
    nsi:manages krlight:ets .

krlight:gloriad-krlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:gloriad.org:2012:gloriad-krlight> ;
    nmleth:vlans "1780-1783" .

krlight:krlight-gloriad a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:gloriad.org:2012:krlight-gloriad> ;
    nmleth:vlans "1780-1783" .

krlight:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

krlight:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

