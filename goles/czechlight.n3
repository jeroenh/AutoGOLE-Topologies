@prefix czechlight: <urn:ogf:network:czechlight2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:czechlight:2012:bi-czechlight-netherlight> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort czechlight:czechlight-netherlight,
        czechlight:netherlight-czechlight .

<urn:ogf:network:czechlight:2012:bi-ps> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort czechlight:ps-in,
        czechlight:ps-out .

<urn:ogf:network:netherlight:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/netherlight.n3" .

czechlight:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "50.088"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "14.421"^^<http://www.w3.org/2001/XMLSchema#float> .

czechlight:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: OpenDRAC (SURFnet) 
            Proj Manager: Michal Altmann  
            email: m.altmann@cesnet.cz 
            phone: +420775603522   
            skype: michal_altmann 
            Software Developer: John MacAuley 
            email: john.macauley@surfnet.nl 
            phone:  1-613-220-6817 
            skype: john.macauley
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://dracproxy01.surfnet.nl:8080/nsi-v1/ConnectionServiceProvider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing <urn:ogf:network:czechlight:2012:ets> .

<urn:ogf:network:czechlight:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nml:hasInboundPort czechlight:netherlight-czechlight,
        czechlight:ps-in;
    nml:hasOutboundPort czechlight:czechlight-netherlight,
        czechlight:ps-out;
    nml:locatedAt czechlight:location;
    nsi:managedBy czechlight:nsa .

czechlight:czechlight-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight:2012:czechlight-netherlight>;
    nmleth:vlans "1780-1783" .

czechlight:netherlight-czechlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight:2012:netherlight-czechlight>;
    nmleth:vlans "1780-1783" .

czechlight:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

czechlight:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

