@prefix exp: <urn:ogf:network:exp.pionier.net.pl:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:pionier.net.pl:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/pionier.net.pl.n3" .

exp:bi-exp-pionier a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort exp:exp-pionier,
        exp:pionier-exp .

exp:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort exp:ps-in,
        exp:ps-out .

exp:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort exp:bi-exp-pionier,
        exp:bi-ps;
    nml:hasInboundPort exp:pionier-exp,
        exp:ps-in;
    nml:hasOutboundPort exp:exp-pionier,
        exp:ps-out;
    nml:locatedAt exp:location;
    nsi:managedBy exp:nsa .

exp:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "52.412"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "16.916"^^<http://www.w3.org/2001/XMLSchema#float> .

exp:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: PSNC 
            Project manager: Radek Krzywania  
            email: radek.krzywania@man.poznan.pl  
            skype: Radoslaw.krzywania 
            Software development: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
             http://194.132.53.174:9282/nsi/ConnectionProvider 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing exp:ets .

exp:exp-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net.pl:2012:exp-pionier>;
    nmleth:vlans "1780-1783" .

exp:pionier-exp a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net.pl:2012:pionier-exp>;
    nmleth:vlans "1780-1783" .

exp:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

exp:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

