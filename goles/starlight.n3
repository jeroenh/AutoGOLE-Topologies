@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix starlight: <urn:ogf:network:starlight.net:2012:> .

starlight:esnet-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:esnet.net:2012:esnet-starlight>;
    nmleth:vlans "1780-1783" .

starlight:gloriad-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:gloriad.net:2012:gloriad-starlight>;
    nmleth:vlans "1780-1783" .

starlight:jgnx-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx.net:2012:jgnx-starlight>;
    nmleth:vlans "1780-1783" .

starlight:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "41.947906"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "-87.655791"^^<http://www.w3.org/2001/XMLSchema#float> .

starlight:max-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:max.net:2012:max-starlight>;
    nmleth:vlans "1780-1783" .

starlight:netherlight-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-starlight>;
    nmleth:vlans "1780-1783" .

starlight:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
             NSA: OpenNSA (NORDUnet) over Argia 
             Project manager: Scott Campbell 
             email: scott.campbell@crc.gc.ca  
             mobile:   
             skype: scottcampbell00 
             Key software engineer: Henrik Thostrup-Jensen 
             email: htj@nordu.net 
             skype: henrik.thostrup.jensen
         """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
             http://nsa.badlab.crc.ca:9080/NSI/services/ConnectionService
         """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing starlight:topology .

starlight:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

starlight:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

starlight:starlight-esnet a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:esnet.net:2012:starlight-esnet>;
    nmleth:vlans "1780-1783" .

starlight:starlight-gloriad a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:gloriad.net:2012:starlight-gloriad>;
    nmleth:vlans "1780-1783" .

starlight:starlight-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx.net:2012:starlight-jgnx>;
    nmleth:vlans "1780-1783" .

starlight:starlight-max a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:max.net:2012:starlight-max>;
    nmleth:vlans "1780-1783" .

starlight:starlight-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:starlight-netherlight>;
    nmleth:vlans "1780-1783" .

starlight:topology a nml:Topology,
        owl:NamedIndividual;
    nml:hasInboundPort starlight:esnet-starlight,
        starlight:gloriad-starlight,
        starlight:jgnx-starlight,
        starlight:max-starlight,
        starlight:netherlight-starlight,
        starlight:ps-in;
    nml:hasOutboundPort starlight:ps-out,
        starlight:starlight-esnet,
        starlight:starlight-gloriad,
        starlight:starlight-jgnx,
        starlight:starlight-max,
        starlight:starlight-netherlight;
    nml:locatedAt starlight:location;
    nsi:managedBy starlight:nsa .

