startap:netherlight-startap a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-startap>;
    nmleth:vlans "1780-1783" .

startap:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

startap:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

startap:startap-esnet a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:esnet:2012:startap-esnet>;
    nmleth:vlans "1780-1783" .

startap:startap-gloriad a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:gloriad:2012:startap-gloriad>;
    nmleth:vlans "1780-1783" .

startap:startap-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx:2012:startap-jgnx>;
    nmleth:vlans "1780-1783" .

startap:startap-max a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:max:2012:startap-max>;
    nmleth:vlans "1780-1783" .

startap:startap-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:startap-netherlight>;
    nmleth:vlans "1780-1783" .

