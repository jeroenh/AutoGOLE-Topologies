@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix startap: <urn:ogf:network:startap.net:2012:> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:es.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/es.net.n3" .

<urn:ogf:network:gloriad.org:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/gloriad.org.n3" .

<urn:ogf:network:jgn-x.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/jgn-x.jp.n3" .

<urn:ogf:network:netherlight.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/netherlight.net.n3" .

startap:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort startap:ps-in,
        startap:ps-out .

startap:bi-startap-es a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort startap:es-startap,
        startap:startap-es .

startap:bi-startap-gloriad a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort startap:gloriad-startap,
        startap:startap-gloriad .

startap:bi-startap-jgn-x a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort startap:jgn-x-startap,
        startap:startap-jgn-x .

startap:bi-startap-netherlight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort startap:netherlight-startap,
        startap:startap-netherlight .

startap:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort startap:es-startap,
        startap:gloriad-startap,
        startap:jgn-x-startap,
        startap:netherlight-startap,
        startap:ps-in ;
    nml:hasOutboundPort startap:ps-out,
        startap:startap-es,
        startap:startap-gloriad,
        startap:startap-jgn-x,
        startap:startap-netherlight ;
    nml:locatedAt startap:location ;
    nsi:managedBy startap:nsa .

startap:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "41.947906"^^xsd:float ;
    nml:long "-87.655791"^^xsd:float .

startap:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
             NSA: OpenNSA (NORDUnet) over Argia 
             Project manager: Scott Campbell 
             email: scott.campbell@crc.gc.ca  
             mobile:   
             skype: scottcampbell00 
             Key software engineer: Henrik Thostrup-Jensen 
             email: htj@nordu.net 
             skype: henrik.thostrup.jensen
         """^^xsd:string ;
    nsi:csProviderEndpoint """
             http://nsa.badlab.crc.ca:9080/NSI/services/ConnectionService
         """^^xsd:string ;
    nsi:manages startap:ets .

startap:es-startap a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:es.net:2012:es-startap> ;
    nmleth:vlans "1780-1783" .

startap:gloriad-startap a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:gloriad.org:2012:gloriad-startap> ;
    nmleth:vlans "1780-1783" .

startap:jgn-x-startap a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:jgn-x.jp:2012:jgn-x-startap> ;
    nmleth:vlans "1780-1783" .

startap:netherlight-startap a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-startap> ;
    nmleth:vlans "1780-1783" .

startap:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

startap:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

startap:startap-es a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:es.net:2012:startap-es> ;
    nmleth:vlans "1780-1783" .

startap:startap-gloriad a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:gloriad.org:2012:startap-gloriad> ;
    nmleth:vlans "1780-1783" .

startap:startap-jgn-x a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:jgn-x.jp:2012:startap-jgn-x> ;
    nmleth:vlans "1780-1783" .

startap:startap-netherlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:startap-netherlight> ;
    nmleth:vlans "1780-1783" .

